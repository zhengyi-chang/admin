{
  "name": "ant-design-pro",
  "version": "5.2.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "dev": "export NODE_OPTIONS=--openssl-legacy-provider && npm run start:test",
    "stage": "export NODE_OPTIONS=--openssl-legacy-provider && npm run start:stage",
    "prod": "export NODE_OPTIONS=--openssl-legacy-provider && npm run start:prod",
    "build:test": "export NODE_OPTIONS=--openssl-legacy-provider && cross-env API_ENV=test umi build",
    "build:stage": "export NODE_OPTIONS=--openssl-legacy-provider && cross-env API_ENV=stage umi build",
    "build:prod": "export NODE_OPTIONS=--openssl-legacy-provider && cross-env API_ENV=prod umi build",
    "serve": "umi-serve",
    "start:test": "cross-env REACT_APP_ENV=test API_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "start:stage": "cross-env REACT_APP_ENV=stage API_ENV=stage MOCK=none UMI_ENV=dev umi dev",
    "start:prod": "cross-env REACT_APP_ENV=prod API_ENV=prod MOCK=none UMI_ENV=dev umi dev",
    "analyze": "cross-env ANALYZE=1 umi build",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"src/**/*\"",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^1.3.4",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^1.1.1",
    "@ant-design/pro-list": "^1.21.64",
    "@antv/data-set": "^0.11.0",
    "@antv/dom-util": "^2.0.0",
    "@antv/g-canvas": "^0.4.12",
    "@antv/g6": "3.8.0",
    "@antv/matrix-util": "^2.0.7",
    "@antv/util": "~2.0.9",
    "@monaco-editor/react": "^4.4.6",
    "@umijs/route-utils": "^2.0.4",
    "@wangeditor/editor": "^5.1.12",
    "@wangeditor/editor-for-react": "^1.0.4",
    "ahooks": "^2.0.0",
    "ali-oss": "^6.17.1",
    "antd": "^4.20.2",
    "antd-etable": "^1.3.71",
    "classnames": "^2.3.0",
    "cos-js-sdk-v5": "^1.4.18",
    "esdk-obs-browserjs": "^3.23.5",
    "lodash": "^4.17.0",
    "lodash-decorators": "^6.0.1",
    "moment": "^2.29.0",
    "monaco-editor": "^0.34.1",
    "numeral": "^2.0.6",
    "omit.js": "^2.0.2",
    "qiniu-js": "^3.4.1",
    "rc-menu": "^9.1.0",
    "rc-util": "^5.16.0",
    "react": "^17.0.0",
    "react-dev-inspector": "^1.7.0",
    "react-dom": "^17.0.0",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.2.0",
    "umi": "^3.5.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.0",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.0",
    "@types/lodash": "^4.14.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.11.1",
    "@umijs/plugin-blocks": "^2.2.0",
    "@umijs/plugin-esbuild": "^1.4.0",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-react": "^2.1.0",
    "@umijs/preset-ui": "^2.2.9",
    "autoprefixer": "^10.4.21",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "eslint": "^7.32.0",
    "jsdom-global": "^3.0.0",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.5.3",
    "prettier": "^2.5.0",
    "stylelint": "^13.0.0",
    "typescript": "^4.5.0",
    "umi-serve": "^1.9.10"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  }
}
